<p id="notice"><%= notice %></p>

<h1>Zipcodedatabases</h1>

<table class="table table-hover table-condensed">
  <thead>
    <tr>
      <th>Zip</th>
      <th>City</th>
      <th>State</th>
      <th>Stateabb</th>
      <th>County</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @zipcodedatabases.each do |zipcodedatabase| %>
      <tr>
        <td><%= zipcodedatabase.zip %></td>
        <td><%= zipcodedatabase.City %></td>
        <td><%= zipcodedatabase.state %></td>
        <td><%= zipcodedatabase.StateAbb %></td>
        <td><%= zipcodedatabase.county %></td>
        <td><%= link_to 'Show', zipcodedatabase, class: "btn btn-link" %></td>
        <td><%= link_to 'Edit', edit_zipcodedatabase_path(zipcodedatabase), class: "btn btn-link" %></td>
        <td><%= link_to 'Destroy', zipcodedatabase, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-link" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Zipcodedatabase', new_zipcodedatabase_path %>
